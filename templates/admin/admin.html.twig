{% extends 'base.html.twig' %}

{% block title %}Administration{% endblock %}

{% block body %}

    <p>Signalements</p>

    {% for newSignal in signalement %}

            <p>Il n'y a rien à ce spot : {{ newSignal.sigVide }}</p>
